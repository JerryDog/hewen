/* =Styleswitcher
----------------------------------------------------------------------------------------------------*/
#styleswitcher {
	position: absolute;
	background: #000;
	margin-top: 5px;
	margin-left: 5px;
	padding: 10px 15px 10px 10px;
	z-index: 9000;
	font-size: 11px;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	/* for IE */
	filter:alpha(opacity=60);
	/* CSS3 standard */
	opacity: 0.8;
	}

#styleswitcher li {
	list-style: none;
	margin-left: 5px;
	}
	
#styleswitcher a {
	color: #888;
	text-decoration: none;
	}

#styleswitcher a:hover {color: #d6d9dd;}
	
